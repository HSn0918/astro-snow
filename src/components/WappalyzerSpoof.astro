<!-- Wappalyzer spoof: hints for tech stack detection -->
<meta name="generator" content={metaContent} />
<meta name="x-tech-stack" content={metaContent} />
<script is:inline>
  (() => {
    const entries = {JSON.stringify(globalEntries)};
    for (const [, globalName] of entries) {
      if (globalName in window) continue;
      if (globalName === 'process') {
        window.process = window.process || { versions: { node: '20.0.0' } };
        continue;
      }
      if (globalName === 'fetch') {
        window.fetch =
          window.fetch ||
          (() => Promise.reject(new Error('Fetch API spoof placeholder')));
        continue;
      }
      window[globalName] = window[globalName] || {};
    }
    document.documentElement.setAttribute('data-tech-stack', {JSON.stringify(metaContent)});
  })();
</script>
